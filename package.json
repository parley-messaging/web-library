{
	"name": "parley-web-library",
	"version": "2.0.0",
	"description": "Front-end solution for Parley",
	"private": true,
	"main": "dist/main/index.js",
	"module": "dist/module/index.js",
	"app": "dist/app/index.js",
	"targets": {
		"app": {
			"context": "browser",
			"isLibrary": false,
			"outputFormat": "commonjs",
			"scopeHoist": true,
			"source": "src/api/index.js",
			"sourceMap": false
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npx parcel serve --host chat-dev.parley.nu --https --cert ssl/ssl.cert --key ssl/ssl.key --no-autoinstall index.html",
		"build": "npx parcel build --target=app src/api/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/parley-messaging/web-library.git"
	},
	"keywords": [
		"parley",
		"messaging",
		"customer",
		"chat"
	],
	"contributors": [
		{
			"name": "Gerben Tesselaar",
			"email": "gerben.tesselaar@parley.nu",
			"url": "https://www.linkedin.com/in/gerben-tesselaar-13758bb8/"
		},
		{
			"name": "Daan Leenders",
			"email": "daan.leenders@parley.nu",
			"url": "https://www.linkedin.com/in/dleenders"
		}
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/parley-messaging/web-library/issues"
	},
	"homepage": "https://github.com/parley-messaging/web-library#readme",
	"dependencies": {
		"@babel/plugin-syntax-jsx": "^7.12.13",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@axe-core/react": "^4.1.1",
		"@babel/cli": "^7.13.16",
		"@babel/core": "^7.13.16",
		"@babel/eslint-parser": "^7.13.14",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"eslint": "^7.24.0",
		"eslint-plugin-compat": "^3.9.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.23.2",
		"parcel": "^2.0.0-beta.3.1"
	},
	"browserslist": [
		"defaults"
	],
	"sideEffects": false
}
